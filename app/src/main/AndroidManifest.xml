<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.uwp.kusd">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".network.VolleyApplication">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewsActivity" />
        <activity android:name=".CalendarActivity" />
        <activity android:name=".LunchActivity" />
        <activity android:name=".SchoolsActivity" />
        <activity android:name=".SocialMediaActivity" />
        <activity android:name=".FeaturesActivity" />
        <activity android:name=".TextAlertActivity" />
        <activity android:name=".BoardMembersActivity"
                  android:parentActivityName=".HomeActivity"
                  android:screenOrientation="portrait"
                  android:theme="@style/AppThemeNoActionBar"/>
        <activity android:name=".InfiniteCampusActivity"/>
    </application>

</manifest>